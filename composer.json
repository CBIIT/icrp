{
    "name": "cbiit/icrp",
    "description": "Website for International Cancer Research Project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "backup_migrate": {
            "type": "vcs",
            "url": "https://github.com/backupmigrate/backup_migrate_core"
        },
        "jqueryvalidate": {
            "type": "package",
            "package": {
                "name": "jqueryvalidate",
                "version": "1.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://jqueryvalidation.org/files/jquery-validation-1.14.0.zip",
                    "type": "zip"
                }
            }
        },
        "arshaw_fullcalendar": {
            "type": "package",
            "package": {
                "name": "arshaw/fullcalendar",
                "version": "3.5.6",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/arshaw/fullcalendar/releases/download/v3.5.1/fullcalendar-3.5.1.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "arshaw/fullcalendar": "3.5.6",
        "box/spout": "^2.7.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "2.2",
        "drupal/admin_toolbar": "1.21",
        "drupal/admin_toolbar_tools": "1.21",
        "drupal/autologout": "1.0",
        "drupal/backup_migrate": "4.0",
        "drupal/bootstrap": "3.7",
        "drupal/bootstrap_layouts": "5.0",
        "drupal/calendar": "1.x-dev",
        "drupal/clientside_validation": "1.0-rc3@RC",
        "drupal/clientside_validation_jquery": "1.0-rc3@RC",
        "drupal/colors": "1.x-dev",
        "drupal/console": "1.0.1",
        "drupal/content_access": "1.x-dev",
        "drupal/core": "8.5.3",
        "drupal/crop": "1.3",
        "drupal/ctools": "3.0",
        "drupal/ctools_block": "3.0",
        "drupal/ctools_views": "3.0",
        "drupal/datatables": "1.0-rc1@RC",
        "drupal/datetime_range_timezone": "1.x-dev",
        "drupal/devel": "1.2",
        "drupal/ds": "3.1",
        "drupal/ds_extras": "3.1",
        "drupal/ds_switch_view_mode": "3.1",
        "drupal/editor_advanced_link": "1.4",
        "drupal/email_registration": "1.0-rc5@RC",
        "drupal/embed": "1.0",
        "drupal/entity": "1.0-beta1@beta",
        "drupal/entity_embed": "1.0-beta2@beta",
        "drupal/entity_notification": "1.0",
        "drupal/examples": "1.x-dev",
        "drupal/extlink": "1.0",
        "drupal/faq": "1.x-dev",
        "drupal/field_permissions": "1.0-beta4@beta",
        "drupal/focal_point": "1.0-beta5@beta",
        "drupal/fontawesome": "2.0",
        "drupal/fullcalendar": "1.x-dev",
        "drupal/fullcalendar_options": "1.x-dev",
        "drupal/google_analytics": "2.2",
        "drupal/honeypot": "1.27",
        "drupal/libraries": "3.x-dev",
        "drupal/linkit": "5.0-beta6@beta",
        "drupal/media_entity": "1.7",
        "drupal/metatag": "1.3",
        "drupal/pathauto": "1.0",
        "drupal/plugin_type_example": "1.x-dev",
        "drupal/recovery_pass": "1.0",
        "drupal/redirect": "1.1",
        "drupal/rules": "3.0.0-alpha3@alpha",
        "drupal/search_exclude": "1.1",
        "drupal/search_kint": "1.0",
        "drupal/security_review": "1.x-dev",
        "drupal/token": "1.0",
        "drupal/twig_tweak": "1.9",
        "drupal/typed_data": "1.0.0-alpha1@alpha",
        "drupal/user_import": "1.x-dev",
        "drupal/views_bootstrap": "3.0",
        "drupal/webform": "5.0-beta25@beta",
        "drupal/webform_analysis": "^1.0@beta",
        "drupal/webform_ui": "5.0-beta25@beta",
        "drupal/webform_views": "5.0-alpha4@alpha",
        "drupal/yaml_content": "^1.0@alpha",
        "drush/drush": "~8.0",
        "fullcalendar/fullcalendar": "^3.5",
        "maennchen/zipstream-php": "^0.4.1",
        "mbostock/d3": "^3.5",
        "phpmailer/phpmailer": "~5.2",
        "phpoffice/phpspreadsheet": "^1.2.0",
        "psr/log": "dev-master",
        "select2/select2": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "~1.4"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes" : true
    },
    "autoload": {
        "classmap": [
            "utility/php/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@composer run drupal-scaffold"
        ],
        "post-update-cmd": [
            "php -r \"copy('themes/bootstrap_subtheme/js/config/fullcalendar.fullcalendar.js', 'modules/fullcalendar/js/fullcalendar.fullcalendar.js');\"",
            "php -r \"shell_exec('drush webform-libraries-download');\""
        ]       
    },
    "extra": {
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/{$name}": [
                "type:drupal-module"
            ],
            "profiles/{$name}": [
                "type:drupal-profile"
            ],
            "themes/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/default/default.services.yml",
                "sites/default/default.settings.php",
                "sites/development.services.yml",
                "sites/example.settings.local.php",
                "sites/example.sites.php"
            ]
        },
        "patches": {
             "drupal/fullcalendar": {
                "Restore AJAX functionality":
                "https://www.drupal.org/files/issues/2828939-18-ajax-fix-combined.patch"
            },
            "drupal/core":{
                "Patch to fix Daterange while using in a view":
                "https://www.drupal.org/files/issues/improve_the_views-2786577-165.patch",
                "Patch to update CKEditor library to 4.9.2":
                "https://www.drupal.org/files/issues/2018-04-19/2962330-2.patch"
            }
        }
    }
}
