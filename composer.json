{
    "name": "cbiit/icrp",
    "description": "Website for International Cancer Research Project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "backup_migrate": {
            "type": "vcs",
            "url": "https://github.com/backupmigrate/backup_migrate_core"
        },
        "jqueryvalidate": {
            "type": "package",
            "package": {
                "name": "jqueryvalidate",
                "version": "1.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://jqueryvalidation.org/files/jquery-validation-1.14.0.zip",
                    "type": "zip"
                }
            }
        },
        "arshaw_fullcalendar": {
            "type": "package",
            "package": {
                "name": "arshaw/fullcalendar",
                "version": "3.5.6",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/arshaw/fullcalendar/releases/download/v3.5.1/fullcalendar-3.5.1.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "arshaw/fullcalendar": "3.5.6",
        "box/spout": "^2.7.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "2.2",
        "drupal/admin_toolbar": "^3.0.3",
        "drupal/admin_toolbar_tools": "^3.0.3",
        "drupal/autologout": "^1.3.0",
        "drupal/backup_migrate": "^5.0.1",
        "drupal/bootstrap": "3.17",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/calendar": "1.x-dev",
        "drupal/captcha": "^1.2.0",
        "drupal/clientside_validation": "^3.0.0-rc4@RC",
        "drupal/clientside_validation_jquery": "^3.0.0-rc4@RC",
        "drupal/colors": "1.x-dev",
        "drupal/console": "1.8.0",
        "drupal/console-core": "1.8.0",
        "drupal/console-en": "1.8.0",
        "drupal/content_access": "1.x-dev",
        "drupal/core": "^8.9",
        "drupal/crop": "^2.1",
        "drupal/csv_serialization": "^2.0.0",
        "drupal/ctools": "^3.7.0",
        "drupal/ctools_block": "^3.7.0",
        "drupal/ctools_views": "^3.7.0",
        "drupal/datatables": "^2.0.0",
        "drupal/datetime_range_timezone": "1.x-dev",
        "drupal/devel": "^2.1",
        "drupal/ds": "^3.6",
        "drupal/ds_extras": "^3.7",
        "drupal/ds_switch_view_mode": "^3.7",
        "drupal/editor_advanced_link": "^1.6",
        "drupal/email_registration": "^1.0",
        "drupal/embed": "^1.3",
        "drupal/entity": "^1.0",
        "drupal/entity_embed": "^1.1",
        "drupal/entity_notification": "1.0",
        "drupal/examples": "1.x-dev",
        "drupal/extlink": "^1.3",
        "drupal/faq": "1.x-dev",
        "drupal/field_permissions": "^1.0",
        "drupal/focal_point": "^1.4",
        "drupal/fontawesome": "^2.15",
        "drupal/fullcalendar": "1.x-dev",
        "drupal/fullcalendar_options": "1.x-dev",
        "drupal/google_analytics": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/libraries": "3.x-dev",
        "drupal/linkit": "^5.0-beta11@beta",
        "drupal/metatag": "^1.13",
        "drupal/panelizer": "^4.4",
        "drupal/panels": "^4.6",
        "drupal/panels_ipe": "^4.6",
        "drupal/pathauto": "^1.8",
        "drupal/plugin_type_example": "1.x-dev",
        "drupal/recaptcha": "^3.0.0",
        "drupal/recovery_pass": "1.0",
        "drupal/redirect": "^1.6",
        "drupal/rules": "^3.0.0-alpha6@alpha",
        "drupal/search_exclude": "^2.0@beta",
        "drupal/search_kint": "1.0",
        "drupal/security_review": "^1.x-dev",
        "drupal/token": "^1.9.0",
        "drupal/twig_tweak": "^1.10",
        "drupal/typed_data": "^1.x-dev",
        "drupal/upgrade_status": "^3.11",
        "drupal/user_import": "1.x-dev",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "3.1",
        "drupal/views_data_export": "^1.0-rc1",
        "drupal/views_templates": "^1.11",
        "drupal/webform": "^6.0.5",
        "drupal/webform_ui": "^6.0.5",
        "drupal/webform_views": "^5.0.0-alpha8@alpha",
        "drupal/yaml_content": "^1.0-alpha7@alpha",
        "drush/drush": "^8",
        "fullcalendar/fullcalendar": "^3.5",
        "league/csv": "^9.1",
        "maennchen/zipstream-php": "^0.4.1",
        "mbostock/d3": "^3.5",
        "phpmailer/phpmailer": "~5.2",
        "phpoffice/phpspreadsheet": "^1.2.0",
        "phpstan/phpstan": "^0.12.99",
        "psr/log": "^1",
        "select2/select2": "^4.0",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "~1.4"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes" : true
    },
    "autoload": {
        "classmap": [
            "utility/php/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@composer run drupal-scaffold"
        ],
        "post-update-cmd": [
            "php -r \"copy('themes/bootstrap_subtheme/js/config/fullcalendar.fullcalendar.js', 'modules/fullcalendar/assets/js/fullcalendar.fullcalendar.js');\"",
            "php -r \"copy('themes/bootstrap_subtheme/js/config/fullcalendar.libraries.js', 'modules/fullcalendar/assets/js/fullcalendar.libraries.js');\"",
            "drush webform-libraries-download",
            "drush updatedb -y",
            "drush cr"
        ]       
    },
    "extra": {
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/{$name}": [
                "type:drupal-module"
            ],
            "profiles/{$name}": [
                "type:drupal-profile"
            ],
            "themes/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/default/default.services.yml",
                "sites/default/default.settings.php",
                "sites/development.services.yml",
                "sites/example.settings.local.php",
                "sites/example.sites.php"
            ]
        },
        "patches": {
            "drupal/fullcalendar": {
                "Patch to fix undefined index: module":
                "https://www.drupal.org/files/issues/fullcalendar-notice_undefined_index_module_in_fullcalendar_legend_preprocess_block-2854562-2.patch"
            },
            "drupal/bootstrap": {
                "Patch to fix unsupported operand types":
                "https://www.drupal.org/files/issues/2019-02-04/3027569-23.patch"
            }
        }
    }
}
