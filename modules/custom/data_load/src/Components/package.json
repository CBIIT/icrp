  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:data-upload-tool": "ng serve --app data-upload-tool",
    "start:import-collaborators": "ng serve --app import-collaborators",
    "start:import-institutions": "ng serve --app import-institutions",
    "build": "npm run build:data-upload-tool && npm run build:import-institutions && npm run build:import-collaborators",
    "build:data-upload-tool": "ng build -prod --app data-upload-tool --progress false --output-hashing none --output-path ../assets/data-upload-tool",
    "build:import-institutions": "ng build -prod --app import-institutions --progress false --output-hashing none --output-path ../assets/import-institutions",
    "build:import-collaborators": "ng build -prod --app import-collaborators --progress false --output-hashing none --output-path ../assets/import-collaborators",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
