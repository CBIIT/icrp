{% set isManager="false" %}
{% for role in user.roles %}
    {% if (role == "manager") or (role == "administrator") %}
        {% set isManager="true" %}
    {% endif %}
{% endfor %}

<div id="events-container">
	<div class="clearfix">
		<h1 style="float:left;">Extneral Resources</h1>
		{% if isManager=="true" %}
			<span style="float:right;">
				<a href="/block/32/?destination=resources" id="icrp-events-edit-link" class="events-edit-link">Edit</a>
			</span>
		{% endif %}
	</div>
	<p>
		{{ drupal_block('externalresources') }}
	</p>
</div>